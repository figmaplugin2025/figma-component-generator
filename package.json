{
  "name": "plugin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:code": "tsc -w --preserveWatchOutput",
    "dev:figma": "concurrently -k -n UI,CODE -c green,blue \"vite\" \"tsc -w --preserveWatchOutput\"",
